doctype html
html(ng-app='mrswift')
  head
    //-<!-- build:css test/compiled/style.min.css -->
    meta(charset='utf-8')
    meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no')
    title Mr.Swift
    link(href='https://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    link(href='vendor/materialize/dist/css/materialize.css', type='text/css', rel='stylesheet', media='screen,projection')
    link(rel='stylesheet', href='css/style.css')
    link(rel='stylesheet', href='css/main.css')
    //-<!-- endbuild -->
  

  body
    include navbar
      
    div(ng-view)
    
    include footer
    
    //-<!-- build:js public/js/scripts.min.js -->
    script(src='vendor/angular/angular.min.js')
    script(src='vendor/angular-route/angular-route.min.js')
    script(src='vendor/angular-resource/angular-resource.min.js')
    script(src='js/main.js')
    script(src='js/services/articleServices.js')
    script(src='js/services/homeServices.js')
    script(src='js/controllers/homeController.js')
    script(src='js/controllers/articlesController.js')
    script(src='js/controllers/articleController.js')
    script(src='js/directives/components.js')
    //-<!-- endbuild -->
    
    
    script(src='vendor/jquery/dist/jquery.min.js')
    script(src='js/lib/upbutton.js')
    script(src='js/lib/scroll.js')
    script(src='vendor/materialize/dist/js/materialize.min.js')
    script(src='js/lib/init.js')
